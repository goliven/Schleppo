oid:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_below="@+id/msgPushSwitch"        android:layout_alignLeft="@+id/textView2"        android:layout_alignStart="@+id/textView2"        android:layout_marginTop="59dp"        android:id="@+id/textView3"        android:textAppearance="@style/TextAppearance.AppCompat.Button"/>    <TextView        android:text="Standort aktivieren"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_centerVertical="true"        android:layout_alignLeft="@+id/textView3"        android:layout_alignStart="@+id/textView3"        android:id="@+id/textView5"/>    <Switch        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_centerVertical="true"        android:layout_alignLeft="@+id/msgPushSwitch"        android:layout_alignStart="@+id/msgPushSwitch"        android:id="@+id/mapActSwitch"        android:textOn="AN"        android:textOff="AUS"/></RelativeLayout>